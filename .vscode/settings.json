{
  "[c]": {
    "files.encoding": "utf8",
    "editor.defaultFormatter": "xaver.clang-format",
    "editor.formatOnSave": true
  },
  "[cpp]": {
    "files.encoding": "utf8",
    "editor.defaultFormatter": "xaver.clang-format",
    "editor.formatOnSave": true
  },
  "[python]":{
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "files.associations": {
    "*.cp": "cpp",
    "algorithm": "cpp",
    "bitset": "cpp",
    "cassert": "cpp",
    "cctype": "cpp",
    "cerrno": "cpp",
    "cfloat": "cpp",
    "ciso646": "cpp",
    "climits": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "complex": "cpp",
    "csetjmp": "cpp",
    "csignal": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "exception": "cpp",
    "fstream": "cpp",
    "functional": "cpp",
    "hash_map": "cpp",
    "hash_set": "cpp",
    "iomanip": "cpp",
    "ios": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "iterator": "cpp",
    "limits": "cpp",
    "list": "cpp",
    "locale": "cpp",
    "map": "cpp",
    "memory": "cpp",
    "new": "cpp",
    "numeric": "cpp",
    "ostream": "cpp",
    "pthread_alloc": "cpp",
    "queue": "cpp",
    "rope": "cpp",
    "set": "cpp",
    "slist": "cpp",
    "sstream": "cpp",
    "stack": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "string": "cpp",
    "strstream": "cpp",
    "typeinfo": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "utility": "cpp",
    "valarray": "cpp",
    "vector": "cpp",
    ".clangd": "yaml",
    "string.h": "c"
  },
  "C/C++ Include Guard.Macro Type": "Filepath",
  "C/C++ Include Guard.Prefix": "",
  "C/C++ Include Guard.Suffix": "",
  "C/C++ Include Guard.Comment Style": "None",
  "C/C++ Include Guard.Path Depth": 1,
  "C/C++ Include Guard.Path Skip": 1,
  "C/C++ Include Guard.Auto Include Guard Insertion For New File": true,
  "C/C++ Include Guard.Insert Blank Line": true,
  "C/C++ Include Guard.Remove Extension": false,
  "C/C++ Include Guard.Subfolder Prefixes": [
    {
      "folderPath": "include/dolphin",
      "prefix": "DOLPHIN_"
    },
    {
      "folderPath": "include/musyx",
      "prefix": "MUSYX_"
    },
    {
      "folderPath": "include/rstl",
      "prefix": "RSTL_"
    },
  ],
  "C/C++ Include Guard.Auto Update Path Allowlist": [
    "include"
  ],
  "C/C++ Include Guard.Auto Update Path Blocklist": [
    "include/zlib"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "config/**/config.json"
      ],
      "url": "./doc/config_schema.json"
    },
    {
      "fileMatch": [
        "config/**/objects.json"
      ],
      "url": "./doc/objects_schema.json"
    }
  ],
  "python.analysis.typeCheckingMode": "standard",
  "cmake.ignoreCMakeListsMissing": true
}
