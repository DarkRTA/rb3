    using ::float_t;

    using ::fabs;
    using ::fmod;

    using ::exp;
    using ::log;
    using ::log10;

    using ::pow;
    using ::sqrt;

    using ::sin;
    using ::cos;
    using ::tan;
    using ::asin;
    using ::acos;
    using ::atan;
    using ::atan2;

    using ::sinh;
    using ::cosh;
    using ::tanh;

    using ::ceil;
    using ::floor;

    using ::frexp;
    using ::ldexp;
    using ::modf;
}

#endif
