    using ::setbuf;
    using ::setvbuf;

    using ::fread;
    using ::fwrite;
    using ::fgetpos;
    using ::fseek;
    using ::fsetpos;
    using ::ftell;
    using ::rewind;

    using ::fgetc;
    using ::fgets;
    using ::fputc;
    using ::fputs;
    using ::getc;
    using ::getchar;
    using ::gets;
    using ::putc;
    using ::putchar;
    using ::puts;
