#ifndef MSL_CPP_CCTYPE_H
#define MSL_CPP_CCTYPE_H
#include <ctype.h>

namespace std {
    using ::isalnum;
    using ::isalpha;
    using ::iscntrl;
    using ::isdigit;
    using ::isgraph;
    using ::islower;
    using ::isprint;
    using ::ispunct;
    using ::isspace;
    using ::isupper;
    using ::isxdigit;

    using ::tolower;
    using ::toupper;
}

#endif
